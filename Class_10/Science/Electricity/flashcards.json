here[
  {"id":1,"front":"What is an electric circuit?","back":"A continuous and closed path of an electric current."},
  {"id":2,"front":"What does a switch do in a circuit?","back":"It makes or breaks the conducting path between the cell and the bulb."},
  {"id":3,"front":"Define electric current.","back":"The rate of flow of electric charge through a conductor."},
  {"id":4,"front":"Which particles carry electric current in metallic conductors?","back":"Electrons."},
  {"id":5,"front":"What is the conventional direction of electric current?","back":"Opposite to the direction of flow of electrons."},
  {"id":6,"front":"Write the formula for electric current.","back":"I = Q/t"},
  {"id":7,"front":"What is the SI unit of electric charge?","back":"Coulomb (C)."},
  {"id":8,"front":"How many electrons make one coulomb of charge?","back":"Approximately 6 × 10^18 electrons."},
  {"id":9,"front":"What is the charge of one electron?","back":"−1.6 × 10⁻¹⁹ C."},
  {"id":10,"front":"What is the SI unit of electric current?","back":"Ampere (A)."},
  {"id":11,"front":"Define one ampere.","back":"One ampere is the current when one coulomb of charge flows per second."},
  {"id":12,"front":"Convert 1 mA into ampere.","back":"1 mA = 10⁻³ A."},
  {"id":13,"front":"Convert 1 µA into ampere.","back":"1 µA = 10⁻⁶ A."},
  {"id":14,"front":"Which instrument is used to measure electric current?","back":"Ammeter."},
  {"id":15,"front":"How is an ammeter connected in a circuit?","back":"In series."},

  {"id":16,"front":"What causes electric charges to flow in a conductor?","back":"Electric potential difference."},
  {"id":17,"front":"Which device produces potential difference?","back":"An electric cell or battery."},
  {"id":18,"front":"Define electric potential difference.","back":"Work done per unit charge to move a charge between two points."},
  {"id":19,"front":"Write the formula for potential difference.","back":"V = W/Q"},
  {"id":20,"front":"What is the SI unit of potential difference?","back":"Volt (V)."},
  {"id":21,"front":"Define one volt.","back":"Potential difference when 1 joule of work is done to move 1 coulomb of charge."},
  {"id":22,"front":"Which instrument measures potential difference?","back":"Voltmeter."},
  {"id":23,"front":"How is a voltmeter connected in a circuit?","back":"In parallel."},
  {"id":24,"front":"In water flow analogy, potential difference is similar to what?","back":"Pressure difference."},

  {"id":25,"front":"State Ohm’s Law.","back":"Potential difference across a conductor is directly proportional to current at constant temperature."},
  {"id":26,"front":"Write Ohm’s Law mathematically.","back":"V = IR"},
  {"id":27,"front":"What is electrical resistance?","back":"The opposition offered by a conductor to the flow of current."},
  {"id":28,"front":"What is the SI unit of resistance?","back":"Ohm (Ω)."},
  {"id":29,"front":"Define one ohm.","back":"Resistance when 1 A current flows at 1 V potential difference."},
  {"id":30,"front":"What is a rheostat?","back":"A variable resistor used to change current in a circuit."},

  {"id":31,"front":"On which factors does resistance depend?","back":"Length, area of cross-section, and nature of material."},
  {"id":32,"front":"How does resistance depend on length?","back":"Directly proportional to length."},
  {"id":33,"front":"How does resistance depend on area of cross-section?","back":"Inversely proportional to area."},
  {"id":34,"front":"Write the formula relating resistance and resistivity.","back":"R = ρl/A"},
  {"id":35,"front":"What is electrical resistivity?","back":"Intrinsic property of a material that resists current."},
  {"id":36,"front":"What is the SI unit of resistivity?","back":"Ohm metre (Ω m)."},
  {"id":37,"front":"Why are alloys used in heating elements?","back":"They have high resistivity and do not oxidise easily."},
  {"id":38,"front":"Which metal is used in bulb filaments?","back":"Tungsten."},
  {"id":39,"front":"Which metals are commonly used for transmission lines?","back":"Copper and aluminium."},

  {"id":40,"front":"What are the two types of resistor combinations?","back":"Series and parallel."},
  {"id":41,"front":"How does current behave in series combination?","back":"Same current flows through all resistors."},
  {"id":42,"front":"How does potential difference behave in series combination?","back":"Total voltage is the sum of individual voltages."},
  {"id":43,"front":"Write the equivalent resistance formula for series.","back":"Rs = R1 + R2 + R3"},
  {"id":44,"front":"How does current behave in parallel combination?","back":"Total current is the sum of branch currents."},
  {"id":45,"front":"How does voltage behave in parallel combination?","back":"Same voltage across each resistor."},
  {"id":46,"front":"Write the equivalent resistance formula for parallel.","back":"1/Rp = 1/R1 + 1/R2 + 1/R3"},
  {"id":47,"front":"Which has lower equivalent resistance: series or parallel?","back":"Parallel combination."},
  {"id":48,"front":"Why are domestic appliances connected in parallel?","back":"Same voltage supply and independent operation."},

  {"id":49,"front":"What is the heating effect of electric current?","back":"Production of heat when current flows through a resistor."},
  {"id":50,"front":"State Joule’s Law of heating.","back":"H = I²Rt"},
  {"id":51,"front":"Name devices based on heating effect.","back":"Electric iron, heater, toaster."},
  {"id":52,"front":"Why are bulb filaments filled with inert gas?","back":"To prevent oxidation and increase life."},
  {"id":53,"front":"What is an electric fuse?","back":"A safety device that protects circuits from excess current."},
  {"id":54,"front":"How is a fuse connected?","back":"In series."},
  {"id":55,"front":"How does a fuse work?","back":"It melts due to heat produced by excess current."},

  {"id":56,"front":"What is electric power?","back":"Rate of consumption of electrical energy."},
  {"id":57,"front":"Write the formula for electric power.","back":"P = VI"},
  {"id":58,"front":"Write alternative formulas for electric power.","back":"P = I²R and P = V²/R"},
  {"id":59,"front":"What is the SI unit of electric power?","back":"Watt (W)."},
  {"id":60,"front":"Define one watt.","back":"Power when 1 A current flows at 1 V."},
  {"id":61,"front":"What is the commercial unit of electrical energy?","back":"Kilowatt hour (kWh)."},
  {"id":62,"front":"1 kWh is equal to how many joules?","back":"3.6 × 10⁶ J."},

  {"id":63,"front":"Why does current decrease when resistance increases?","back":"Current is inversely proportional to resistance."},
  {"id":64,"front":"Which combination is safer for household wiring?","back":"Parallel combination."},
  {"id":65,"front":"Why should ammeter have low resistance?","back":"So it does not change circuit current."},
  {"id":66,"front":"Why should voltmeter have high resistance?","back":"So it draws negligible current."},
  {"id":67,"front":"Which effect of current is used in electric fuse?","back":"Heating effect."},
  {"id":68,"front":"Which law relates current, voltage and resistance?","back":"Ohm’s Law."},
  {"id":69,"front":"What happens to resistance of a conductor with rise in temperature?","back":"Resistance increases."},
  {"id":70,"front":"Why are thick wires used in household circuits?","back":"To reduce resistance and heating."}
]
